package sqliteDataBase.Model;

public class Time {
	private int hour;
	private int minute;
	private int second;
	
	public int getHour(){
		return this.hour;
	}
	public void setHour(int hour){
		this.hour = hour;
	}
	
	public int getMinute(){
		return this.minute;
	}
	public void setMinute(int minute){
		this.minute = minute;
	}
	
	public int getSecond(){
		return this.second;
	}
	public void setSecond(int second){
		this.second = second;
	}
	

}
